<struts>
    <package name="cardType" extends="struts-default">
           <action name="cardType_*" method="{1}" class="control.CardTypeController"> 
               <result name="success">/cardType.jsp</result>
               <result name="modifyQuery">/cardTypeModify.jsp</result>
